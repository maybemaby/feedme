		"drizzle-seed": "^0.3.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.4.0",
		"jsdom": "^27.0.0",
		"mode-watcher": "^1.1.0",
		"prettier": "^3.6.2",
